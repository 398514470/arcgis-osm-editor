<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    @*http://ajax.aspnetcdn.com/ajax/jQuery/jquery-1.5.1.min.js*@
    @*http://www.asp.net/ajaxlibrary/cdn.ashx*@
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>    
</head>
<body>
    <div class="page">
        <header id="header">
            <img alt="OpenStreetMap Editor Logo" id="osmLogo" height="64" width="64" src="@Url.Content("~/Images/osm_logo.png")" />
            <h1>ArcGIS Editor for OpenStreetMap </h1>
            <div class="version">Version 2.0 Beta 3</div>
            
            <div id="logindisplay">
            <li>@Html.ActionLink("Logs", "Index", "Logs")</li>
               @* @Html.Partial("_LogOnPartial")*@
            </div>
            <nav>
                <ul id="menu">
                    <li>@Html.ActionLink("Logs", "Index", "Logs")</li>
                   @* <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("About", "About", "Home")</li>*@
                </ul>
            </nav>
        </header>
        <section id="main">
            @RenderBody()
        </section>
        <footer>
        
        </footer>
    </div>
    <div class="footer">&copy; Esri, <a href="http://www.esri.com/" style="color: #FFF">http://www.esri.com/</a></div>
</body>
</html>
